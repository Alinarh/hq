<h4>Студенты</h4>

<%= render 'filters' %>

<%= params.inspect %>

<%= paginate @students %>

<table class="table table-hover">
  <thead>
  <tr>
    <th>#</th>
    <th>Фамилия</th>
    <th>Имя</th>
    <th>Отчество</th>
  </tr>
  </thead>
  <tbody>
  <% @students.each do |student| %>
  <tr>
    <td><%= student.id %></td>
    <td><%= student.last_name %></td>
    <td><%= student.first_name %></td>
    <td><%= student.patronym %></td>
  </tr>
  <% end %>
  </tbody>
</table>