<div class="page-header">
  <h1>Все оценки</h1>
</div>

<div class="row">
  <table class="table table-hover">
    <thead>
    <tr>
      <th>#</th>
      <th>Студент</th>
      <th>Результат</th>
    </tr>
    </thead>
    <tbody>
    <% @marks.each do |mark| %>
      <tr>
        <td><%= mark.id %></td>
        <td><%= mark.student_id %></td>
        <td><%= mark.mark %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
 </div> 

<%= link_to 'Добавить', new_session_mark_path, class: 'btn btn-default' %>

