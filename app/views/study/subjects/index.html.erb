<div class="page-header">
  <h1>Все результаты</h1>
</div>

<%= link_to 'Добавить', new_study_subject_path, class: 'btn btn-default' %>

<table class="table table-hover">
  <thead>
  <tr>
    <th>#</th>
    <th>Год</th>
    <th>Семестр</th>
    <th>Группа</th>
    <th>Дисциплина</th>
    <th>Форма отчетности</th>
  </tr>
  </thead>
  <tbody>
  <% @subjects.each do |subject| %>
    <tr>
      <td><%= subject.id %></td>
      <td><%= "#{subject.year}/#{subject.year + 1}" %></td>
      <td><%= subject.semester %></td>
      <td><%= subject.group.name %></td>
      <td><%= subject.title %></td>
      <td><%= subject.type %></td>
    </tr>
  <% end %>
  </tbody>
</table>
